<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox id="screen_bg" fx:id="screen_bg" alignment="BOTTOM_RIGHT" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="screen" alignment="CENTER" text="0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox id="calculation_bg" alignment="TOP_RIGHT">
         <children>
            <Label id="calculations" fx:id="calculations" text="0+0">
               <HBox.margin>
                  <Insets bottom="20.0" right="20.0" />
               </HBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <VBox id="buttons_container" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="DEL" mnemonicParsing="false" onAction="#onActionPressed" prefHeight="50.0" prefWidth="50.0" text="DEL" HBox.hgrow="ALWAYS" />
                  <Button fx:id="LOG" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onActionPressed" prefHeight="50.0" prefWidth="50.0" text="LOG" />
                  <Button fx:id="AC" layoutX="88.0" layoutY="10.0" mnemonicParsing="false" onAction="#onActionPressed" prefHeight="50.0" prefWidth="50.0" text="AC" HBox.hgrow="ALWAYS" />
                  <Button fx:id="DIVIDE" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" onAction="#onOperatorPressed" prefHeight="50.0" prefWidth="50.0" text="/" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="one" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="1" />
                  <Button fx:id="two" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="2" />
                  <Button fx:id="three" layoutX="88.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="3" />
                  <Button fx:id="MULTIPLY" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" onAction="#onOperatorPressed" prefHeight="50.0" prefWidth="50.0" text="Ã—" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="four" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="4" />
                  <Button fx:id="five" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="5" />
                  <Button fx:id="six" layoutX="88.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="6" />
                  <Button fx:id="ADDITION" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" onAction="#onOperatorPressed" prefHeight="50.0" prefWidth="50.0" text="+" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="seven" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="7" />
                  <Button fx:id="eight" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="8" />
                  <Button fx:id="nine" layoutX="88.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="9" />
                  <Button fx:id="SUBTRACT" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" onAction="#onOperatorPressed" prefHeight="50.0" prefWidth="50.0" text="-" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="RESULT" mnemonicParsing="false" onAction="#onResultPressed" prefHeight="50.0" prefWidth="110.0" text="RESULT" />
                  <Button fx:id="zero" alignment="CENTER" layoutX="88.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="0" />
                  <Button fx:id="DECIMAL" layoutX="75.0" layoutY="15.0" mnemonicParsing="false" onAction="#onNumberPressed" prefHeight="50.0" prefWidth="50.0" text="." />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</VBox>
